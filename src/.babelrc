{
  "presets": [
    [
      "@babel/preset-env", {
        "modules":  false
      }
    ],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties", {
        "loose": true
      }
    ]
  ],
  "ignore": [
    "node_modules/**"
  ]
}